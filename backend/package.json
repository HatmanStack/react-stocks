{
  "name": "react-stocks-backend",
  "version": "1.0.0",
  "description": "AWS Lambda backend for React Stocks application - proxies Tiingo and Polygon APIs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:aws-sdk",
    "build:watch": "npm run build -- --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "clean": "rm -rf dist coverage",
    "warm-cache": "tsx scripts/warm-cache.ts",
    "create-dashboard": "./scripts/create-dashboard.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:guided": "./scripts/deploy.sh --guided",
    "validate": "./scripts/validate.sh",
    "logs": "./scripts/logs.sh"
  },
  "keywords": [
    "aws-lambda",
    "api-gateway",
    "serverless",
    "stocks",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.931.0",
    "@aws-sdk/lib-dynamodb": "^3.931.0",
    "axios": "^1.13.2",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.17.0",
    "@types/sentiment": "^5.0.4",
    "esbuild": "^0.24.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.20.6",
    "typescript": "^5.6.0"
  }
}
